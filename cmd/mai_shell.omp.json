{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "\ue285 ",
    "foreground": "#FEF5ED",
    "background": "transparent"
  },
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "display_host": true
          },
          "leading_diamond": "<#ff70a6> \ue200 </>",
          "template": "{{ .UserName }} <#ffffff>on</>",
          "foreground": "#ffbebc",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "Monday <#ffffff>at</> 3:04 PM"
          },
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#bc93ff",
          "type": "time",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "environment",
            "fetch_virtual_env": true,
            "home_enabled": true
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }}{{ end }} ",
          "foreground": "#FFDE57",
          "powerline_symbol": "\ue0b0",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#ee79d1",
          "type": "git",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "foreground": "#a9ffb4",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "dallas",
            "threshold": 0
          },
          "template": " {{ .FormattedMs }}s <#ffffff>\ue601</>",
          "foreground": "#a9ffb4",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "root_icon": "\uf292 "
          },
          "template": " \uf0e7 ",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " <#ffffff>CPU:</> {{ round .PhysicalPercentUsed .Precision }}% ",
          "foreground": "#94ffa2",
          "type": "sysinfo",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ffffff>\ue266</> <#ffffff>RAM:</> {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB ",
          "foreground": "#81ff91",
          "type": "sysinfo",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf07b",
            "folder_separator_icon": " \uebcb ",
            "home_icon": "home",
            "style": "agnoster_full"
          },
          "leading_diamond": "<#00c7fc> \ue285 </><#ffafd2>{</>",
          "trailing_diamond": "<#ffafd2>}</>",
          "template": " \ue5ff {{ .Path }} ",
          "foreground": "#ffafd2",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " {{ if gt .Code 0 }}\ue795{{ else }}\ue795{{ end }} ",
          "foreground": "#A9FFB4",
          "type": "exit",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
